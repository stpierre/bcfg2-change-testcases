<!-- clients.xml:

     The Metadata/clients.xml file contains the mappings of Profile
     Groups to clients. The file is just a series of <Client /> tags,
     each of which describe one host.

-->

<Clients xmlns:xi="http://www.w3.org/2001/XInclude">
	<xi:include href="upstreamclients.xml"/>
	<Client name="client1" profile="default"/>
	<Client name="client2" profile="default"/>
	<Client name="client3" profile="profile2"/>
	<Client name="client4" profile="profile2"/>
	<Client name="bcfg1" profile="default"/>
	<Client name="bcfg2" profile="profile2"/>
</Clients>
